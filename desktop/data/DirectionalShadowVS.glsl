//http://www.lighthouse3d.com/tutorials/glsl-tutorial/directional-lights-per-pixel///estos atributos vienen del archivo .OBJattribute vec4 a_position;attribute vec4 a_color;attribute vec2 a_texCoord0;attribute vec4 a_normal;//estos uniforms son seteados por mi aplicacionuniform mat4 u_vp;uniform mat4 u_m;varying vec4 v_position;//iluminacionvarying vec4 v2f_normalW;uniform vec3 EyePosW3;uniform vec3 LightPosW_3; // Light's position in world space.uniform vec3 MaterialDiffuse_3;varying vec4 v2f_positionW;//este es mi Vertex Shader void main(){   v_position = u_m*vec4(a_position, 1.0);	gl_Position = u_vp *a_position;}