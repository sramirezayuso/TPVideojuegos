//http://www.microbasic.net/tutorials/shadow-mapping/ShadowMapping.pdf//estos atributos vienen del archivo .OBJattribute vec4 a_position;attribute vec4 a_color;attribute vec2 a_texCoord0;attribute vec4 a_normal;//estos uniforms son seteados por mi aplicacionuniform mat4 u_mvp_cam;// de la camarauniform mat4 u_mvp_light;//mvp de la luzvarying vec4 v_position;varying vec4 w_position;void main(){		gl_Position= u_mvp_cam *a_position;		w_position=u_mvp_light*a_position;}